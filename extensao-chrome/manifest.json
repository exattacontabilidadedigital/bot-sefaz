{
  "manifest_version": 3,
  "name": "SEFAZ-MA Auto Login",
  "version": "1.0.0",
  "description": "Preenche automaticamente login do SEFAZ-MA",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "*://sefaznet.sefaz.ma.gov.br/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://sefaznet.sefaz.ma.gov.br/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
