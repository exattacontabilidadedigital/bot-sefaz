{
  "manifest_version": 3,
  "name": "Portal SEFAZ Automator",
  "version": "2.0.0",
  "description": "Automatizador de consultas no Portal SEFAZ - ID Renovado 2025",
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "*://sefaznet.sefaz.ma.gov.br/*",
    "*://sefaz.ma.gov.br/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://sefaznet.sefaz.ma.gov.br/*",
        "*://sefaz.ma.gov.br/*"
      ],
      "js": ["content-simple.js"],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "background-simple.js"
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:3000/*",
      "http://localhost:8000/*",
      "http://127.0.0.1:3000/*",
      "http://127.0.0.1:8000/*"
    ]
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Portal SEFAZ Automator"
  },
  "key": ""
}
