# Configurações do Bot SEFAZ
# Credenciais de acesso
USUARIO=seu_usuario_aqui
SENHA=sua_senha_aqui

# Configurações do banco de dados
DB_PATH=sefaz_consulta.db

# Configurações do navegador
HEADLESS=false
TIMEOUT=30000

# URL do sistema
SEFAZ_URL=https://sefaznet.sefaz.ma.gov.br/sefaznet/login.do?method=prepareLogin

# SMTP (opcional) - configurar para enviar notificações por e-mail
SMTP_HOST=smtp.exemplo.com
SMTP_PORT=587
SMTP_USER=seu_usuario_smtp
SMTP_PASS=sua_senha_smtp
SMTP_FROM=no-reply@seu-dominio.com
SMTP_TLS=true
NOTIFY_TO=fiscal@exattacontabilidade.com.br